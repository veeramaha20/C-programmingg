#include <stdio.h>

int main() {

    int a, b, c, d, e;

    int sum;

    float average;

    // Read 5 integers

    scanf("%d %d %d %d %d", &a, &b, &c, &d, &e);

    // Calculate sum and average

    sum = a + b + c + d + e;

    average = sum / 5.0;

    // Print result

    printf("Sum of the 5 number is : %d\n", sum);

    printf("Average of the 5 number is : %.1f\n", average);

    return 0;

}
